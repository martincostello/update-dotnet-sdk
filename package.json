{
  "name": "update-dotnet-sdk",
  "version": "2.2.1",
  "private": true,
  "description": "A GitHub Action that updates the .NET SDK.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/**/*.ts",
    "format-check": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --minify --source-map --license licenses.txt",
    "test": "jest --coverage",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martincostello/update-dotnet-sdk.git"
  },
  "keywords": [
    "actions",
    "dotnet",
    "github",
    "sdk"
  ],
  "author": {
    "name": "Martin Costello",
    "url": "https://martincostello.com/"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/martincostello/update-dotnet-sdk/issues"
  },
  "homepage": "https://github.com/martincostello/update-dotnet-sdk#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@actions/http-client": "2.1.0",
    "@actions/io": "^1.1.3"
  },
  "devDependencies": {
    "@octokit/types": "^9.2.2",
    "@typescript-eslint/parser": "^5.59.6",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.40.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^28.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.8",
    "ts-jest": "^28.0.8",
    "typescript": "^5.0.4"
  }
}
