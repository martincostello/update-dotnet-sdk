{
  "name": "update-dotnet-sdk",
  "version": "2.4.2",
  "private": true,
  "description": "A GitHub Action that updates the .NET SDK.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "check-dist": "npm run build && npm run package",
    "format": "prettier --write src/**/*.ts tests/**/*.ts",
    "format-check": "prettier --check src/**/*.ts tests/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --minify --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martincostello/update-dotnet-sdk.git"
  },
  "keywords": [
    "actions",
    "dotnet",
    "github",
    "sdk"
  ],
  "author": {
    "name": "Martin Costello",
    "url": "https://martincostello.com/"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "reporters": [
      "default",
      "github-actions"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "verbose": true
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/martincostello/update-dotnet-sdk/issues"
  },
  "homepage": "https://github.com/martincostello/update-dotnet-sdk#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@actions/http-client": "2.1.1",
    "@actions/io": "^1.1.3"
  },
  "devDependencies": {
    "@octokit/types": "^11.1.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.47.0",
    "eslint-plugin-github": "^4.9.2",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.3",
    "prettier": "^3.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
